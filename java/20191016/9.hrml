	   System.out.println(stud1.getName());
   }
}
class Studentx{
   String studNo,name;
   public void setStudNo(String studNo) {
      this.studNo=studNo;
   }
   public void setName(String name) {
      this.name=name;
   }
   public String getStudNo() {
      return this.studNo;
   }
   public String getName() {
      return this.name;
   }
}
